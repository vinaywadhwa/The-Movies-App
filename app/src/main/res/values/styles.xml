<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textViewStyle">@style/CustomTextStyle</item>
        <item name="actionBarPopupTheme">@style/PopupOverlay</item>
    </style>

    <style name="PopupOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="iconTint">#E6EAEC</item>
    </style>

    <style name="CustomTextStyle" parent="android:Widget.Material.TextView">
        <item name="android:textColor">?colorAccent</item>
    </style>
    <style name="roundedImageView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">18dp</item>
    </style>


</resources>